<mapper namespace="query">

   <select id="getMasterCode" parameterType="com.project.vo.MasterCodeVo" resultType="com.project.vo.MasterCodeVo">
     <![CDATA[
        SELECT FACTORY_CD
			, BCODE
			, SCODE
			, CODE_NM
			, SORT_ORDER
			, BIGO
			, USE_YN
			, WRITE_DT
			, WRITE_ID
			, UPDATE_DT
			, UPDATE_ID
		FROM MCC001
    ]]>
  </select>
 

</mapper>
